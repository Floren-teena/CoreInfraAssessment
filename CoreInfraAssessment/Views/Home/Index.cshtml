    //code for star functionalities
    $(document).ready(function () {
        
        $('#select-all').click(function () {
            var isSelected = $(this).hasClass('selected'); 
            if (isSelected) {
                
                $(this).removeClass('selected'); 
                $('.star').not(this).removeClass('fa-solid').addClass('fa-regular'); 
            } else {
               
                $(this).addClass('selected'); 
                $('.star').not(this).removeClass('fa-regular').addClass('fa-solid'); 
            }
        });

        // Individual row selection
        $('.star').not('#select-all').click(function () {
            var $this = $(this);
            var isSelected = $this.hasClass('fa-solid'); 
            if (isSelected) {
                
                $this.removeClass('fa-solid').addClass('fa-regular');
            } else {
                
                $this.addClass('fa-solid').removeClass('fa-regular');
            }

            
            if ($('.star.fa-regular').length > 0) {
                $('#select-all').removeClass('selected'); 
            } else {
                $('#select-all').addClass('selected');
            }
        });
    });

</script>