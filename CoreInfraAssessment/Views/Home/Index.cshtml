        [18, 57, 95, 73, 89, 25, 24, 18, 57, 95, 73, 89],
        [25, 90, 70, 90, 25, 85, 70, 87, 25, 90, 25, 85], 
        [30, 50, 30, 25, 30, 25, 30, 50, 30, 25, 30, 25],
        [35, 78, 40, 20, 59, 27, 35, 78, 40, 20, 59, 27],
        [20, 30, 25, 35, 30, 40, 35, 57, 95, 73, 89, 25],
        [25, 90, 70, 90, 25, 85, 70, 87, 25, 90, 25, 85],
    ];

    // Function to create the mini charts
    function createMiniChart(chartId, data) {
        var ctx = document.getElementById(chartId).getContext('2d');
        new Chart(ctx, {
            type: 'line',
            data: {
                labels: ['', '', '', '', '', '', '', '', '', '', '', ''], 
                datasets: [{
                    data: data,
                    borderColor: '#6183ef',
                    borderWidth: 2,
                    backgroundColor: 'rgba(54, 162, 235, 0.1)', 
                    fill: true, 
                    pointRadius: 0.1,
                    pointHoverRadius: 5, 
                    pointBackgroundColor: 'white', 
                    pointBorderColor: '#6183ef', 
                    pointBorderWidth: 2,
                    hoverBorderWidth: 5,
                    tension: 0.4
                }]
            },
            options: {
                responsive: false,
                maintainAspectRatio: false,
                plugins: {
                    legend: { display: false }, 
                    tooltip: {
                        enabled: true, 
                        mode: 'nearest',
                        intersect: false,
                        backgroundColor: '#6183ef',
                        callbacks: {
                            label: function () {
                                return ''; 
                            },
                        },
                    },
                },
                hover: {
                    mode: 'nearest', 
                    intersect: true
                },
                scales: {
                    x: { display: false }, 
                    y: { display: false }  
                }
            }
        });
    }

    
    createMiniChart('miniChart1', chartData[0]);
    createMiniChart('miniChart2', chartData[1]);
    createMiniChart('miniChart3', chartData[2]);
    createMiniChart('miniChart4', chartData[3]);
